datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = []
}

model Conversion {
  id              String   @id @default(uuid())
  amount          Decimal
  baseCurrency    String
  targetCurrency  String
  conversionRate  Decimal
  convertedAmount Decimal
  createdAt       DateTime @default(now())
}
